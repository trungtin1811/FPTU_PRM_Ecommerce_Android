<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.ecommerceproject.activities.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.example.ecommerceproject.activities.ui.cart.CartFragment"
        tools:layout="@layout/fragment_cart" />

    <fragment
        android:id="@+id/navigation_order"
        android:name="com.example.ecommerceproject.activities.ui.order.OrderFragment"
        tools:layout="@layout/fragment_order" />

    <fragment
        android:id="@+id/navigation_user_information"
        android:name="com.example.ecommerceproject.activities.ui.userinformation.UserInformationFragment"
        tools:layout="@layout/fragment_user_information" />
</navigation>